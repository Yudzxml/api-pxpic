{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ytmp3",
      "dest": "api/ytmp3.js"
    },
    {
      "src": "/api/remini",
      "dest": "api/remini.js"
    },
    {
      "src": "/api/tiktok",
      "dest": "api/tiktok.js"
    },
    {
      "src": "/api/igdl",
      "dest": "api/igdl.js"
    },
    {
      "src": "/api/update",
      "dest": "api/update.js"
    }
  ]
}
